package pasteleria;

public class PastelMulticapa extends PastelEstandar{

	public PastelMulticapa(IntPastel pastel) {
		super(pastel);
		// TODO Auto-generated constructor stub
	}

	@Override
	public String decorar() {
		// TODO Auto-generated method stub
		return super.decorar()+" multicapa";
	}

	@Override
	public String tipo() {
		// TODO Auto-generated method stub
		return null;
	}
	
	public int setPrecio() {
		return super.getPRECIO()+5;
	}

}
